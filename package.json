{
  "name": "my-express-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../rust-crypto-analyzer && npm install",
    "build:rust": "cd rust-crypto-analyzer && npm run build",
    "build:server": "tsc",
    "build:ui": "cd frontend && npm run build",
    "build": "npm run build:rust && npm run build:server && npm run build:ui",
    "start": "node dist/app.js",
    "dev:server": "nodemon --respawn --transpile-only src/app.ts",
    "dev:ui": "cd frontend && npm start",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:ui\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
